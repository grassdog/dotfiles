tap 'homebrew/bundle'
tap 'homebrew/core'
tap 'homebrew/services'

brew 'aspell'
brew 'awscli'
brew 'ctags'
brew 'diff-so-fancy'
brew 'ripgrep'
brew 'fzf'
brew 'yarn'
brew 'graphviz'

brew 'dnsmasq', restart_service: :changed

brew 'asdf'
brew 'figlet'
brew 'git'
brew 'gnupg2'
brew 'libxml2'
brew 'gnu-sed'
brew 'heroku'
brew 'httpie'
brew 'imagemagick'
brew 'jq'
brew 'keybase'
brew 'macvim'
brew 'openssl'
brew 'pandoc'

brew 'postgresql', restart_service: :changed

brew 'pstree'
brew 'python'
brew 'reattach-to-user-namespace', args: [ 'with-wrap-pbcopy-and-pbpaste' ]
brew 'sqlite'
brew 'tmux'
brew 'trash'
brew 'tree'
brew 'watchman'
brew 'wget'
brew 'youtube-dl'
brew 'z'
brew 'zsh'
brew 'vim', args: [ 'with-ruby', 'with-mzscheme', 'with-python' ]
brew 'emacs', args: ['with-cocoa', 'with-gnutls', 'with-imagemagick', 'with-dbus', 'with-modules', 'with-mailutils' ]
